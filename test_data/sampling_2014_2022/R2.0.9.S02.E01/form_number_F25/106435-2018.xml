               <AC_CRITERION>Evidence of social and environmental capabilities (80)</AC_CRITERION>
               <AC_CRITERION>Financial Offer (80)</AC_CRITERION>
               <AC_CRITERION>Evidence of regulatory compliance (50)</AC_CRITERION>
               <DURATION TYPE="MONTH">240</DURATION>
               <NO_EU_PROGR_RELATED/>
            </OBJECT_DESCR>

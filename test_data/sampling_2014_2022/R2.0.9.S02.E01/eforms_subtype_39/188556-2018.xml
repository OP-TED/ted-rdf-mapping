            <TITLE>
               <P>Vanasadama välikoristus 2017 - 2020</P>
            </TITLE>
            <REFERENCE_NUMBER>191407</REFERENCE_NUMBER>
            <CPV_MAIN>
               <CPV_CODE CODE="90620000"/>

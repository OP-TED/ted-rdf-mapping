# Organization - Company
tedm:Organisation a rr:TriplesMap ;
    rdfs:label "ND-Company";
    rdfs:comment "Applies to: GR-Company";
    rr:subjectMap
        [
            rr:template "http://data.europa.eu/a4g/resource/id_{replace(replace(/TED_EXPORT/CODED_DATA_SECTION/NOTICE_DATA/NO_DOC_OJS, ' ', '-' ), '/' , '-')}_Organisation_{unparsed-text('https://digest-api.ted-data.eu/api/v1/hashing/fn/uuid/' || encode-for-uri(path()) || '?response_type=raw')}" ;
            rr:class org:Organization

        ] ;
    rml:logicalSource
        [
            rml:source "data/source.xml" ;
            rml:iterator "/*/ext:UBLExtensions/ext:UBLExtension/ext:ExtensionContent/efext:EformsExtension/efac:Organizations/efac:Organization";
            rml:referenceFormulation ql:XPath
        ] ;
    rr:predicateObjectMap
        [
            rdfs:comment "The address of the company";
            rr:predicate cv:registeredAddress ;
            rr:objectMap
                [
                    rr:parentTriplesMap tedm:OrganisationAddress;
                    rr:joinCondition [
                        rr:child "path(.)";
                        rr:parent "path(../..)";
                    ];
                ]
        ] ;
    rr:predicateObjectMap
        [
            rdfs:comment "The contact point of the company";
            rr:predicate epo:hasPrimaryContactPoint ;
            rr:objectMap
                [
                    rr:parentTriplesMap tedm:ContactPoint ;
                    rr:joinCondition [
                        rr:child "path(.)";
                        rr:parent "path(../..)";
                    ];
                ]
        ] ;
    rr:predicateObjectMap
        [
            rdfs:comment "The identification information of the company";
            rr:predicate epo:hasID;
            rr:objectMap
                [
                    rr:parentTriplesMap tedm:OrganisationIdentifier;
                    rr:joinCondition [
                        rr:child "path(.)";
                        rr:parent "path(../..)";
                    ];
                ]
        ] ;
    rr:predicateObjectMap
        [
            rdfs:comment "The endpoint ID and the website URI of the company";
            rr:predicate epo:hasDeliveryGateway ;
            rr:objectMap
                [
                    rr:parentTriplesMap tedm:OrganisationChannel ;
                    rr:joinCondition [
                        rr:child "path(.)";
                        rr:parent "path(..)";
                    ];
                ]
        ] ;
    rr:predicateObjectMap
        [
            rdfs:comment "A property that indicates the natural person that is related with the company";
            rdfs:label "BT-633-Organization";
            rr:predicate foaf:name ;
            rr:objectMap
                [
                    rml:reference "efbc:NaturalPersonIndicator"
                ] ;
        ] ;
    rr:predicateObjectMap
        [
            rdfs:comment "A property that indicates the legal name of a company";
            rdfs:label "BT-500-Organization-Company";
            rr:predicate epo:hasLegalName ;
            rr:objectMap
                [
                    rml:reference "efac:Company/cac:PartyName/cbc:Name"
                ] ;
        ] .

tedm:OrganisationChannel a rr:TriplesMap ;

            rr:predicate epo:hasMainActivity ;
            rr:objectMap
                [
                    rr:parentTriplesMap tedm:mainActivity;
                    rr:joinCondition [
                        rr:child "../CA_ACTIVITY/@VALUE";
                        rr:parent "xmlElement";
                    ];
                ] ;
        ] ;

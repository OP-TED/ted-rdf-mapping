  epo:definesContractDuration <http://data.europa.eu/a4g/resource/Duration/2021-S-216-569914/9b5f500b-480f-339a-b633-57d0799d4507>;
  epo:definesSpecificPlaceOfPerformance <http://data.europa.eu/a4g/resource/ContractTermLocation/2021-S-216-569914/507edaeb-912d-3eb4-a32d-da9c8c650bd8>;
  epo:hasEInvoicing true;

#                    ];
#                ] ;
        ]  ;
    rr:predicateObjectMap
        [
            rdfs:label "BT-10-Procedure-Buyer";
            rr:predicate   epo:hasMainActivity ;
            rr:objectMap
                [
                    rml:reference "../cac:ContractingActivity/cbc:ActivityTypeCode[@listName='authority-activity']";
                ] ;

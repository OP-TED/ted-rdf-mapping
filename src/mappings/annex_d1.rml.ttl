                [
                    rml:reference "if (exists(PT_AWARD_CONTRACT_WITHOUT_CALL/D_ACCORDANCE_ARTICLE/D_MANUF_FOR_RESEARCH)) then 'http://publications.europa.eu/resource/authority/direct-award-justification/resd' else null";

#Lot
tedm:Lot a rr:TriplesMap ;
    rdfs:label "ND-Lot";
    rml:logicalSource
        [
            rml:source "data/source.xml" ;
            rml:iterator "/*/cac:ProcurementProjectLot[cbc:ID/@schemeName='Lot']";
            rml:referenceFormulation ql:XPath
        ];
    rr:subjectMap
        [
            rr:template "http://data.europa.eu/a4g/resource/id_{replace(replace(/*/cbc:ID[@schemeName='notice-id'], ' ', '-' ), '/' , '-')}_Lot_{unparsed-text('https://digest-api.ted-data.eu/api/v1/hashing/fn/uuid/' || encode-for-uri(path()) || '?response_type=raw')}" ;
            rr:class epo:Lot
        ] ;
    rr:predicateObjectMap
        [
            rdfs:label "GR-Lot-SubmissionTool";
            rr:predicate epo:usesChannel ;
            rr:objectMap
                [
                    rr:parentTriplesMap tedm:LotChannel ;
                    rr:joinCondition [
                        rr:child "path(.)";
                        rr:parent "path(../.)";
                    ];
                ] ;
        ] ;
    rr:predicateObjectMap
        [
            rdfs:label "GR-Lot-PublicOpeningEvent";
            rr:predicate epo:isSubjectToLotSpecificTerm ;
            rr:objectMap
                [
                    rr:parentTriplesMap tedm:LotOpeningTerm ;
                    rr:joinCondition[
                        rr:child "path(.)";
                        rr:parent "path(../..)"
                    ];
                ] ;
        ] ;
    rr:predicateObjectMap
        [
            rdfs:label "GR-Lot-Deadlines1; GR-Lot-SubmissionMethod";
            rr:predicate epo:isSubjectToLotSpecificTerm ;
            rr:objectMap
                [
                    rr:parentTriplesMap tedm:SubmissionProcess ;
                    rr:joinCondition [
                        rr:child "path(.)";
                        rr:parent "path(../.)";
                    ];
                ] ;
        ] ;
    rr:predicateObjectMap
        [
            rdfs:label "GR-Lot-Deadlines1; GR-Lot-SubmissionMethod";
            rr:predicate epo:isSubjectToLotSpecificTerm ;
            rr:objectMap
                [
                    rr:parentTriplesMap tedm:SubmissionTerm ;
                    rr:joinCondition [
                        rr:child "path(.)";
                        rr:parent "path(../.)";
                    ];
                ] ;
        ] ;
    rr:predicateObjectMap
        [
            rdfs:label "GR-Lot-PostAwardProcess";
            rr:predicate epo:foreseesContractSpecificTerm ;
            rr:objectMap
                [
                    rr:parentTriplesMap tedm:ContractTerm ;
                    rr:joinCondition [
                        rr:child "path(.)";
                        rr:parent "path(../.)";
                    ];
                ] ;
        ] ;
    rr:predicateObjectMap
        [
            rdfs:label "ND-Lot-Requirements2SecurityClearance1; ND-Lot-SecurityClearance2";
            rr:predicate epo:isSubjectToLotSpecificTerm ;
            rr:objectMap
                [
                    rr:parentTriplesMap tedm:SecurityTerm ;
                    rr:joinCondition [
                        rr:child "path(.)";
                        rr:parent "path(../.)";
                    ];
                ] ;
        ] ;
    rr:predicateObjectMap
        [
            rdfs:label "ND-AwardingTerms";
            rr:predicate epo:isSubjectToLotSpecificTerm ;
            rr:objectMap
                [
                    rr:parentTriplesMap tedm:AwardingTerm ;
                    rr:joinCondition [
                        rr:child "path(.)";
                        rr:parent "path(../..)";
                    ];
                ] ;
        ] ;
    rr:predicateObjectMap
        [
            rdfs:label "BT-71-Lot";
            rr:predicate epo:hasReservedProcurement  ;
            rr:objectMap
                [
                    rml:reference "descendant::cbc:TendererRequirementTypeCode[@listName='reserved-procurement']";
                ] ;
        ] .


